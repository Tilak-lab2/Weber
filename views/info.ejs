<table class="table table-stripped table-primary">
<thead class="thead-dark">
  <tr>
  
  <th>Name</th>

  <th>Quantity</th>
  <th>Price</th>
  <th>Stock</th>
  </tr>
</thead>
<% if(locals.user){%> 
<% records.forEach(function(row) {%>
<tr>
 
 <td><%=user.name%></td>
 
 <td><%= row.quantity%></td>
 <td><%=row.unitPrice%></td>
 <td><%=row.unitStock%></td>
</tr> 

<%})%>


<%}%>
