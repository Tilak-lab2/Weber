<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title></title>
    

</head>
<body>
   

        <div class="container">
            <div class="row">
                <h1>Thank You</h1>
            <!-- <div class="box-nav d-flex justify-between"> -->
            </div>
            </div> 
   
    
    <table class="table table-stripped table-primary">
   <thead class="thead-dark">
      <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Description</th>
      <th>Quantity</th>
      <th>Price</th>
      </tr>
   </thead>
   <% if(locals.user){%> 
   <% records.forEach(function(row) {%>
    <tr>
     <td><%= row._id %></td>
     <td><%=row.name%></td>
     <td><%= row.desc %></td>
     <td><%= row.quantity%></td>
     <td><%=row.price%></td>
     <td><a href="/api/edit/<%= row._id %> " class="btn btn-primary btn-xs"><i class="fa fa-edit"></i></a></td>
     <td><a href="/api/delete/<%= row._id %>" class="btn btn-danger btn-xs"><i class="fa fa-trash"></i></a></td>
     
   </tr>
    </table>
 <% }) %>
<%}%>
 </div>
 </div>
 </main>


</body>
</html>