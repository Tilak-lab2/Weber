<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title></title>
    

</head>
<body>
   

        <div class="container">
            <div class="row">
                <h1>Thank You</h1>
            <!-- <div class="box-nav d-flex justify-between"> -->
            </div>
            </div> 
   
    <div class="table-responsive">
    <table class="table table-bordered table-primary">
        <caption>Product Details</caption>
        <thead class="thead-dark">
      <tr>
      
      <th>ProductName</th>
      <th>User name</th>
      <th>Quantity</th>
      <th>Price</th>
      <th>InStock</th>
      <th>Update</th>
      <th>Remove</th>
      </tr>
   </thead>
   <% if(locals.user){%> 
    
    <% records.forEach(function(row){ %>
    
        <tr>
       <td> <%=row.name%></td>
       <td><%=user.name%></td>
       <td><%=row.quantity%></td>
       <td><%=row.unitPrice%></td>
       <td><%=row.unitStock%></td>
       <td>
         <a href="/api/edit/<%= row._id %> " class="btn btn-primary btn-s"><i class="fa fa-edit"></i></a></td>
       <td><a href="/api/delete/<%= row._id %>" class="btn btn-danger btn-s"><i class="fa fa-trash"></i></a></td>
 </tr>      
 </tr>
    </table>
    </div>
 

        <%})%>
   
   <%}%>

</body>
</html>